      <form class="navbar-form pull-right">
        <input type="text" class="span10" placeholder="validation key">
        <button type="submit" class="btn">Submit</button>
      </form>
    </div>
    <div class="span10">
      <div class="container">
        <!-- main page -->
        <%= yield %>
